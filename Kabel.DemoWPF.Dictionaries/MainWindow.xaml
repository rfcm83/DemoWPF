<Window x:Class="Kabel.DemoWPF.Dictionaries.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="Header" Style="{StaticResource HeaderStyle}"/>
            <TextBlock Text="SubHeader" Style="{StaticResource SubHeaderStyle}"/>
            <TextBlock Text="Normal" Style="{StaticResource NormalStyle}"/>
            <TextBlock Text="Bold" Style="{StaticResource BoldStyle}"/>
            <TextBlock Text="Italic" Style="{StaticResource ItalicStyle}"/>
        </StackPanel>
    </Grid>
</Window>
